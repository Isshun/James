<level-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="oval" >
            <solid android:color="#f00" />

            <size
                android:height="20dp"
                android:width="20dp" />
        </shape>
    </item>
    <item android:top="10dp">
        <shape android:shape="rectangle" >
            <solid android:color="#f00" />
        </shape>

        <size
            android:height="10dp"
            android:width="20dp" />
    </item>

</level-list>